<Window x:Class="WordPad_Kasianova.SaveWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WordPad_Kasianova"
        mc:Ignorable="d"
        Title="SaveWindow" Height="200" Width="350"
        FontFamily="Courier New"
        Background="AliceBlue"
        FontSize="22">
    <StackPanel x:Name="TitleWindow">
        <Label Content="Введите название" />
        <TextBox x:Name="Title" Margin="10"/>
        <Button x:Name="TitleButton"
            Width="200"
            Margin="10"
            Command=""
            Content="OK"
            Click="Ok_Click"/>
    </StackPanel>
</Window>
